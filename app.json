{
  "expo": {
    "name": "restaurante",
    "slug": "restaurante",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.restaurante"
    },
    "android": {
      "package": "com.restaurante.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.7.20",
            "extraMavenRepos": [
              "https://maven.google.com"
            ],
            "gradleVersion": "7.6.1"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "086975a8-966a-4146-a723-fe5d4a17b4a2"
      }
    }
  }
}
